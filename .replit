modules = ["c-clang14", "cpp-clang14", "bash"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["zlib", "pkg-config", "libxcrypt"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "MUD Server"

[[workflows.workflow]]
name = "MUD Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "./start_mud.sh"

[workflows.workflow.metadata]
outputType = "console"

[[ports]]
localPort = 5000
externalPort = 80
